{
  "name": "exbico/jsend-response",
  "description": "JSend implementation in php",
  "type": "library",
  "license": "MIT",
  "homepage": "https://bitbucket.org/exbico/jsendresponse",
  "keywords": ["JSend"],
  "minimum-stability": "stable",
  "prefer-stable": true,
  "authors": [
    {
      "name": "Yurij",
      "email": "koptev@exbico.ru"
    },
    {
      "name": "Ezytli",
      "email": "putilin@exbico.ru"
    },
    {
      "name": "AxelPAL",
      "email": "palchikov@exbico.ru"
    }
  ],
  "require": {
    "php": "^8.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^10.0",
    "squizlabs/php_codesniffer": "^3.7"
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": {
      "*": "dist"
    },
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "Exbico\\JsendResponse\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "tests\\": "tests/"
    }
  }
}
